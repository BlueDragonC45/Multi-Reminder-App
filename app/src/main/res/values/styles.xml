<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    //style (1)
    <style name="testStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#DC3B72</item>
        <item name="colorPrimaryDark">#A3160C</item>
        <item name="colorAccent">#C7D547</item>
    </style>

    //style (2)
    <style name="Dark" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#2F3DBC</item>
        <item name="colorPrimaryDark">#151895</item>
        <item name="colorAccent">#000000</item>

</resources>